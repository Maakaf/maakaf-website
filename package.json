{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "npx prettier --write . --config ./.prettierrc",
    "prepare": "husky install",
    "test": "npx playwright test --ui",
    "test-result": "npx playwright show-report"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json,scss}": [
      "next lint",
      "npx prettier --write . --config ./.prettierrc"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.8",
    "classnames": "^2.3.2",
    "embla-carousel-autoplay": "^8.0.0-rc12",
    "embla-carousel-react": "^8.0.0-rc12",
    "eslint": "8.43.0",
    "framer-motion": "^10.16.2",
    "lucide-react": "^0.276.0",
    "mongoose": "^8.3.4",
    "next": "^14.1.4",
    "next-intl": "^3.9.3",
    "next-themes": "0.2.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-tooltip": "^5.21.3",
    "sharp": "^0.33.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "pretty-quick": "^3.1.3"
  }
}
