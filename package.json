{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "npx prettier --write . --config ./.prettierrc",
    "prepare": "husky install",
    "test:ui": "npx playwright test --ui",
    "test": "npx playwright install && npx playwright test",
    "prepush": "npm run test",
    "test-result": "npx playwright show-report",
    "precommit": "lint-staged && npm run lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json,scss}": [
      "next lint",
      "npx prettier --write . --config ./.prettierrc"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "autoprefixer": "10.4.14",
    "classnames": "^2.3.2",
    "embla-carousel-autoplay": "^8.0.0-rc12",
    "embla-carousel-react": "^8.0.0-rc12",
    "eslint": "8.43.0",
    "framer-motion": "^10.16.2",
    "lucide-react": "^0.276.0",
    "next": "^14.2.3",
    "next-intl": "^3.9.3",
    "next-themes": "0.2.1",
    "postcss": "^8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-tooltip": "^5.21.3",
    "sharp": "^0.33.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@playwright/test": "^1.42.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/nextjs": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "pretty-quick": "^3.1.3",
    "storybook": "^8.1.1"
  }
}
